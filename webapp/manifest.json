{
  "_version": "1.17.0",
  "sap.app": {
    "id": "rgssoa",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:worklist",
      "version": "1.9.3",
      "toolsId": "5bf5554a-673f-4617-a0ed-d69eb2b18fbb"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZS4_SOA_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ZSS4_CA_CONI_VISIBILITA_SRV": {
        "uri": "/sap/opu/odata/sap/ZSS4_CA_CONI_VISIBILITA_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "annotations": [],
          "localUri": "localService/ZSS4_CA_CONI_VISIBILITA_SRV/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "fullWidth": true,
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.71.50",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "rgssoa.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "ZSS4_CA_CONI_VISIBILITA_SRV": {
        "dataSource": "ZSS4_CA_CONI_VISIBILITA_SRV",
        "preload": true,
        "settings": {
          "defaultBindingMode": "OneWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css",
          "id": "stylecss"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "rgssoa.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": ["notFound"]
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "startpage",
          "target": ["startpage"]
        },
        {
          "pattern": "AuthPage",
          "name": "authPage",
          "target": ["authPage"]
        },
        {
          "pattern": "SOAPage",
          "name": "SOAPage",
          "target": ["SOAPage"]
        },
        {
          "pattern": "AutorizzazioneSet/{objectId}",
          "name": "object",
          "target": ["object"]
        },
        {
          "pattern": "AuthDetail/{Bukrs},{Gjahr},{Zchiaveaut},{ZstepAut}",
          "name": "authDetail",
          "target": ["authDetail"]
        },
        {
          "pattern": "RegisterSoa",
          "name": "registerSoa",
          "target": ["registerSoa"]
        },
        {
          "pattern": "DocumentiCosto",
          "name": "documentiCosto",
          "target": ["documentiCosto"]
        },
        {
          "pattern": "NoDocumentiCosto",
          "name": "noDocumentiCosto",
          "target": ["noDocumentiCosto"]
        }
      ],
      "targets": {
        "startpage": {
          "viewName": "StartPage",
          "viewId": "StartPage",
          "viewLevel": 1,
          "title": "{i18n>worklistViewTitle}"
        },
        "authPage": {
          "viewName": "AuthPage",
          "viewId": "authPage",
          "viewLevel": 2,
          "title": "{i18n>authPageViewTitle}"
        },
        "SOAPage": {
          "viewName": "SOAPage",
          "viewId": "soaPage",
          "viewLevel": 2,
          "title": "{i18n>soaPageViewTitle}"
        },
        "authDetail": {
          "viewName": "AuthDetail",
          "viewId": "authDetail",
          "viewLevel": 2,
          "title": "{i18n>authDetailTitle}"
        },

        "object": {
          "viewName": "Object",
          "viewId": "object",
          "viewLevel": 2,
          "title": "{i18n>objectViewTitle}"
        },
        "objectNotFound": {
          "viewName": "ObjectNotFound",
          "viewId": "objectNotFound"
        },
        "notFound": {
          "viewName": "NotFound",
          "viewId": "notFound"
        },
        "registerSoa": {
          "viewName": "RegisterSoa",
          "viewId": "registerSoa",
          "viewLevel": 3
        },
        "documentiCosto": {
          "viewName": "DocumentiCosto",
          "viewId": "documentiCosto",
          "viewLevel": 4
        },
        "noDocumentiCosto": {
          "viewName": "NoDocumentiCosto",
          "viewId": "noDocumentiCosto",
          "viewLevel": 4
        }
      }
    },
    "rootView": {
      "viewName": "rgssoa.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    }
  }
}
