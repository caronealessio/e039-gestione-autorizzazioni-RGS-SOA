<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
  xmlns:l="sap.ui.layout"
>
 <Panel 
      id="pnlNewQuietanzante"
      backgroundDesign="Transparent"
      visible="{NewModalitaPagamento>/VisibleNewQuietanzante}"
      >
    <l:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="0.5">
      <Label 
        text="{i18n>inputCognome}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
        required="true"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Label>
      <Input
        value="{NewQuietanzante>/Zqcognome}"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Label 
        text="{i18n>inputNome}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
        required="true"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Label>
      <Input
        value="{NewQuietanzante>/Zqnome}"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputCF}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewQuietanzante>/Stcd1}"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputQualifica}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewQuietanzante>/Zqqualifica}"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>

      <Text 
        text="{i18n>inputDataNascita}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <DatePicker
        value="{NewQuietanzante>/Zqdatanasc}"
        width="100%"
        valueFormat="yyyy-MM-dd"
        displayFormat="dd.MM.yyyy"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </DatePicker>
      <Text 
        text="{i18n>inputLuogoNascita}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewQuietanzante>/Zqluogonasc}"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputProvinciaNascita}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
        
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewQuietanzante>/Zqprovnasc}"
        maxLength="3"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputTelefono}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input 
        type="Number"
        value="{NewQuietanzante>/Zqtelefono}"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>

      <Text 
        text="{i18n>inputIndirizzo}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewQuietanzante>/Zqindiriz}"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputCitta}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewQuietanzante>/Zqcitta}"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputCap}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        type="Number"
        value="{NewQuietanzante>/Zqcap}"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputProvinciaResidenza}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewQuietanzante>/Zqprovincia}"
        maxLength="3"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
    </l:Grid>
  </Panel>
</c:FragmentDefinition>