<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
<SelectDialog
		id="sdBeneficiario"
    noDataText="{i18n>noBeneficiario}"
		title="{i18n>columnNameBeneficiario}"
		search="onValueHelpChange"
		confirm="onValueHelpBeneficiarioClose"
		cancel="onValueHelpBeneficiarioClose"
    app:input="fltBeneficiario"
		app:searchPropertyModel="Beneficiario"
		items="{path: 'Beneficiario>/'}">		
		<StandardListItem
      iconDensityAware="false"
      iconInset="false"
      title="{Beneficiario>Beneficiario}"
      app:key="{Beneficiario>Beneficiario}"
      app:typeBen="{Beneficiario>Type}"
      app:name="{Beneficiario>Nome}"
      app:surname="{Beneficiario>Cognome}"
      app:ragSociale="{Beneficiario>RagSoc}"
      app:codFiscale="{Beneficiario>CodFisc}"
      app:codFiscaleEstero="{Beneficiario>CodFiscEst}"
      app:pIva="{Beneficiario>PIva}"
    />
	</SelectDialog>
</c:FragmentDefinition>