<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
<SelectDialog
		id="sdCodiceTributo"
    noDataText="{i18n>noCodiceTributo}"
		title="{i18n>labelCodiceTributo}"
		search="onValueHelpChange"
		confirm="onValueHelpCodiceTributoClose"
		cancel="onValueHelpCodiceTributoClose"
		items="{path: 'CodiceTributo>/'}">		
		<StandardListItem
      iconDensityAware="false"
      iconInset="false"
      title="{CodiceTributo>Zcodtrib}"
    />
	</SelectDialog>
</c:FragmentDefinition>