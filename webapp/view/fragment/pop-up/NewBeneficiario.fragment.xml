<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
<Dialog 
  id="dlgNewBeneficiario"
  contentWidth="100%"
  title="{i18n>titleNewBeneficiario}"
  
>
  <content>
    <c:Fragment fragmentName="rgssoa.view.fragment.panel.NewBeneficiarioDatiAnagrafica" type="XML"/>
    <c:Fragment fragmentName="rgssoa.view.fragment.panel.NewBeneficiarioDatiFiscali" type="XML"/>
    <c:Fragment fragmentName="rgssoa.view.fragment.panel.NewBeneficiarioDatiPagamento" type="XML"/>
  </content>

  <buttons>
    <Button text="{i18n>btnBack}" press="onBackNewBeneficiario" type="Emphasized"/>
    <Button text="{i18n>btnSave}" press="onSaveNewBeneficiario" type="Emphasized"/>
  </buttons>
</Dialog>
</c:FragmentDefinition>